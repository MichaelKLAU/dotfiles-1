[color "diff"]
        meta = yellow bold
        frag = magenta
        old = red
        new = green 
        whitespace = white reverse
        commit = blue reverse
        plain = black
[color "status"]
        added = yellow
        changed = green
        untracked = cyan reverse
        branch = magenta
[push]
	default = simple
[alias]
	st = status -sb
        l = "!source ~/.githelper && pretty_git_log"
        la = "!git l --all"
        lr = !git l -50 --graph
        lg = !git l --graph
        lar = !git lr --all
	lp = log --patch --decorate
